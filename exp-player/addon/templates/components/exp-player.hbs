<div class="row" id="experiment-player">
    <div class="col-md-8">
        {{
        component currentFrameTemplate
        frameIndex=frameIndex
        frameConfig=currentFrameConfig

        saveHandler=(action 'saveFrame')
        next=(action 'next') last=(action 'last') previous=(action 'previous')
        }}
    </div>
    <div class="col-md-4 pull-right">
        {{#unless isFullScreen}}
            <button class="btn btn-warning pull-right" {{ action 'showFullscreen' }}>Return to fullscreen</button>
        {{/unless}}
    </div>
</div>