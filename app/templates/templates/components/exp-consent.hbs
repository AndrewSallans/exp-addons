<div class="jumbotron">
  <h1>{{ title }}</h1>
  <hr>
  <p> {{ body }}</p>
  <hr >
  <div class="input-group">    
    <span class="input-group-addon">
      {{ consentLabel }}
    </span>
    {{input type="checkbox" checked=consentGranted}}
  </div>
</div>
<div class="row exp-controls">
  <div class="btn-group">
    <button class="btn btn-default pull-right" {{ action 'next' }} disabled={{ consentNotGranted }}>
      Continue
    </button>
  </div>
</div>
